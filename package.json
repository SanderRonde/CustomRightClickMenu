  "scripts": {
    "pretest": "yarn pretest:main && yarn pretest:dist && yarn pretest:post",
    "pretest:main": "gulp stashBrowser && gulp browserChrome && gulp cleanDist && gulp buildTest",
    "pretest:dist": "yarn pretest:dist:chrome-copy && yarn pretest:dist:firefox-copy && yarn pretest:dist:edge-copy && yarn pretest:dist:opera-copy",
    "pretest:dist:chrome-copy": "gulp browserChromeBuild && gulp chromeToDistTest",
    "pretest:dist:firefox-copy": "gulp browserFirefoxBuild && gulp firefoxToDistTest",
    "pretest:dist:edge-copy": "gulp browserEdgeBuild && gulp edgeToDistTest",
    "pretest:dist:opera-copy": "gulp browserOperaBuild && gulp operaToDistTest",
    "pretest:post": "yarn package && gulp unstashBrowser && gulp copyUnstashed",
    "test": "yarn test:global && yarn test:chrome && yarn test:firefox && yarn test:edge",
    "test:global": "mocha test/test.js",
    "test:chrome": "yarn test:chrome:old && yarn test:chrome:latest",
    "test:chrome:old": "yarn test:chrome:old:page && yarn test:chrome:old:page:stylesheet && yarn test:chrome:old:extension && yarn test:chrome:old:install",
    "test:chrome:old:page": "yarn test:chrome:old:page:pre-stylesheet && test:chrome:old:page:stylesheet && yarn test:chrome:old:page:post-stylesheet",
    "test:chrome:old:page:pre-stylesheet": "mocha test/UITest.js --chrome-26 --skip-type-stylesheet --skip-contextmenu",
    "test:chrome:old:page:stylesheet": "mocha test/UITest.js --chrome-26 --skip-types-except-stylesheet --skip-non-dialogs --skip-contextmenu",
    "test:chrome:old:page:post-stylesheet": "mocha test/UITest.js --chrome-26 --skip-entrypoints",
    "test:chrome:old:extension": "yarn test:chrome:old:extension:pre-stylesheet && yarn test:chrome:old:extension:stylesheet && yarn test:chrome:old:extension:post-stylesheet",
    "test:chrome:old:extension:pre-stylesheet": "mocha test/UITest.js --chrome-40 --skip-type-stylesheet --skip-contextmenu --test-extension",
    "test:chrome:old:extension:stylesheet": "mocha test/UITest.js --chrome-40 --skip-types-except-stylesheet --skip-non-dialogs --skip-contextmenu  --test-extension",
    "test:chrome:old:extension:post-stylesheet": "mocha test/UITest.js --chrome-40 --skip-entrypoints --test-extension",
    "test:chrome:old:install:": "yarn test:chrome:old:install:greasyfork && yarn test:chrome:old:install:openuserjs && yarn test:chrome:old:install:userscriptsorg && yarn test:chrome:old:install:userstylesorg",
    "test:chrome:old:install:greasyfork": "mocha test/installation-test.js --chrome-40 --test greasyfork",
    "test:chrome:old:install:openuserjs": "mocha test/installation-test.js --chrome-40 --test openuserjs",
    "test:chrome:old:install:userscriptsorg": "mocha test/installation-test.js --chrome-40 --test userscripts.org",
    "test:chrome:latest": "yarn test:chrome:latest:page && yarn test:chrome:latest:extension",
    "test:chrome:latest:page": "yarn test:chrome:latest:page:pre-stylesheet && test:chrome:latest:page:stylesheet && yarn test:chrome:latest:page:post-stylesheet",
    "test:chrome:latest:page:pre-stylesheet": "mocha test/UITest.js --chrome-latest --skip-type-stylesheet --skip-contextmenu",
    "test:chrome:latest:page:stylesheet": "mocha test/UITest.js --chrome-latest --skip-types-except-stylesheet --skip-non-dialogs --skip-contextmenu",
    "test:chrome:latest:page:post-stylesheet": "mocha test/UITest.js --chrome-latest --skip-entrypoints",
    "test:chrome:latest:extension": "yarn test:chrome:latest:extension:pre-stylesheet && yarn test:chrome:latest:extension:stylesheet && yarn test:chrome:latest:extension:post-stylesheet",
    "test:chrome:latest:extension:pre-stylesheet": "mocha test/UITest.js --chrome-latest --skip-type-stylesheet --skip-contextmenu --test-extension",
    "test:chrome:latest:extension:stylesheet": "mocha test/UITest.js --chrome-latest --skip-types-except-stylesheet --skip-non-dialogs --skip-contextmenu  --test-extension",
    "test:chrome:latest:extension:post-stylesheet": "mocha test/UITest.js --chrome-latest --skip-entrypoints --test-extension",
    "test:chrome:latest:install:": "yarn test:chrome:latest:install:greasyfork && yarn test:chrome:latest:install:openuserjs && yarn test:chrome:latest:install:userscriptsorg && yarn test:chrome:latest:install:userstylesorg",
    "test:chrome:latest:install:greasyfork": "mocha test/installation-test.js --chrome-latest --test greasyfork",
    "test:chrome:latest:install:openuserjs": "mocha test/installation-test.js --chrome-latest --test openuserjs",
    "test:chrome:latest:install:userscriptsorg": "mocha test/installation-test.js --chrome-latest --test userscripts.org",
    "test:chrome:latest:install:userstylesorg": "mocha test/installation-test.js --chrome-latest --test userstyles.org",
    "test:chrome:latest:install:openusercss": "mocha test/installation-test.js --chrome-latest --test openusercss",
    "test:firefox": "yarn test:firefox:quantum && yarn test:firefox:latest",
    "test:firefox:quantum": "mocha test/UITest.js --firefox-quantum",
    "test:firefox:latest:pre-on-page": "mocha test/UITest.js --firefox-latest --skip-contextmenu",
    "test:firefox:latest:on-page": "mocha test/UITest.js --firefox-latest --skip-entrypoints",
    "test:firefox:latest": "yarn test:firefox:latest:pre-on-page && yarn test:firefox:latest:on-page",
    "test:edge": "yarn test:edge:16 && yarn test:edge:latest",
    "test:edge:16": "mocha test/UITest.js --edge-16",
    "test:edge:latest": "mocha test/UITest.js --edge-latest",
    "build": "yarn build:main && yarn build:chrome-copy && yarn build:firefox-copy && yarn build:edge-copy && yarn build:opera-copy && yarn build:post",
    "build:main": "gulp stashBrowser && gulp browserChrome && gulp cleanDist && gulp build",
    "build:chrome-copy": "gulp browserChromeBuild && gulp chromeToDist",
    "build:firefox-copy": "gulp browserFirefoxBuild && gulp firefoxToDist",
    "build:edge-copy": "gulp browserEdgeBuild && gulp edgeToDist",
    "build:opera-copy": "gulp browserOperaBuild && gulp operaToDist",
    "build:post": "gulp unstashBrowser && gulp copyUnstashed",
    "build-chrome": "gulp stashBrowser && gulp browserChrome && gulp build && gulp chromeToDist",
    "build-firefox": "gulp stashBrowser && gulp browserFirefox && gulp build && gulp firefoxToDist",
    "build-edge": "gulp stashBrowser && gulp browserEdge && gulp build && gulp edgeToDist",
    "build-opera": "gulp stashBrowser && gulp browserOpera && gulp build && gulp operaToDist",
    "dist": "yarn build && yarn package",
    "test-local": "yarn pretest && yarn test:global",
    "test-build": "gulp stashBrowser && gulp browserChrome && gulp testBuild",
    "genAppx:gen": "cd dist && manifoldjs -l debug -p edgeextension -f edgeextension -m edge/manifest.json",
    "genAppx:sign": "manifoldjs -l debug -p edgeextension package dist/CRM/edgeextension/manifest/",
    "genAppx:move": "node -e \"require('fs').renameSync('dist/CRM/edgeextension/package/edgeExtension.appx', 'dist/packed/Custom Right-Click Menu.edge.appx')\"",
    "genAppx:clean": "gulp genAppx:clean",
    "genAppx:finish": "yarn genAppx:move && yarn genAppx:clean",
    "genAppx:create": "yarn genAppx:gen && gulp genAppx:copy && gulp genAppx:replace && yarn genAppx:sign",
    "genAppx": "node scripts/has-edge-secrets.js && (yarn genAppx:create && yarn genAppx:finish) || echo \"Edge secrets not present, skipping edge packaging\"",
    "package": "gulp genCRX && gulp genXPI && yarn genAppx",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'app/bower_components', 'junction') } catch (e) { }\""
  },
  "version": "2.2.7",
  "dependencies": {}
